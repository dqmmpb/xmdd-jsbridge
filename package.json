{
  "private": true,
  "engines": {
    "node": ">=0.12.0"
  },
  "devDependencies": {
    "babel-core": "^5.5.6",
    "babel-preset-es2015": "~6.6.0",
    "babel-register": "~6.7.2",
    "browser-sync": "^2.2.1",
    "cz-conventional-changelog": "^1.1.5",
    "del": "^1.1.1",
    "event-stream": "^3.3.4",
