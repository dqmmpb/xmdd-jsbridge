    "gulp-autoprefixer": "^2.3.1",
    "gulp-buffer": "0.0.2",
    "gulp-cache": "^0.2.8",
    "gulp-clean-css": "^2.0.1",
    "gulp-css-url-adjuster": "^0.2.3",
    "gulp-eslint": "^0.13.2",
    "gulp-filter": "^3.0.1",
    "gulp-gzip": "^1.2.0",
    "gulp-htmlmin": "^1.2.0",
    "gulp-iconfont": "^6.0.0",
    "gulp-iconfont-css": "^2.1.0",
    "gulp-if": "^1.2.5",
    "gulp-imagemin": "^2.2.1",
    "gulp-less": "^3.0.5",
    "gulp-load-plugins": "^0.10.0",
    "gulp-plumber": "^1.0.1",
    "gulp-px2rem": "~1.0.8",
    "gulp-rename": "^1.2.2",
    "gulp-rev": "^7.1.2",
    "gulp-rev-collector": "^1.0.5",
    "gulp-rev-hash": "0.0.2",
    "gulp-sequence": "^0.4.5",
    "gulp-size": "^1.2.1",
    "gulp-sourcemaps": "^1.5.0",
    "gulp-tar": "~1.8.0",
    "gulp-uglify": "^1.1.0",
    "gulp-useref": "^3.0.0",
    "main-bower-files": "^2.5.0",
    "opn": "^1.0.1",
    "require-dir": "^0.3.0",
    "wiredep": "^2.2.2"
  },
  "eslintConfig": {
    "env": {
      "node": true,
      "browser": true,
      "jquery": true
    },
    "globals": {
      "WebKitCSSMatrix": true,
      "AMap": true,
      "webkitRequestAnimationFrame": true,
      "mozRequestAnimationFrame": true,
      "webkitCancelAnimationFrame": true,
      "mozCancelAnimationFrame": true,
      "wx": true,
      "React": true,
      "ReactDOM": true,
      "FastClick": true,
      "connectWebViewJavascriptBridge": true,
      "WebViewJavascriptBridge": true
    },
    "rules": {
      "quotes": [
        2,
        "single"
      ],
      "new-cap": [
        2,
        {
          "newIsCap": false,
          "capIsNew": false
        }
      ],
      "no-unused-vars": false,
      "curly": false,
      "no-underscore-dangle": false,
      "consistent-return": false,
      "camelcase": false
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
